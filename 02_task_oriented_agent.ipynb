    "def debug_tool_execution(agent, tool_name, **params):\n",
    "    \"\"\"\n",
    "    Debug tool execution by showing detailed information.\n",
    "    \"\"\"\n",
    "    print(f\"\\n=== Debugging Tool: {tool_name} ===\")\n",
    "    print(f\"Parameters: {params}\")\n",
    "    \n",
    "    if tool_name not in agent.tools:\n",
    "        print(f\"Error: Tool '{tool_name}' not found\")\n",
    "        return\n",
    "    \n",
    "    tool = agent.tools[tool_name]\n",
    "    print(f\"Tool Description: {tool.description}\")\n",
    "    print(f\"Tool Schema: {json.dumps(tool.get_schema(), indent=2)}\")\n",
    "    \n",
    "    result = agent.execute_tool(tool_name, **params)\n",
    "    print(f\"Execution Result: {json.dumps(result, indent=2)}\")\n",
    "    \n",
    "    return result\n",
    "\n",
    "# Debug calculator tool\n",
    "debug_tool_execution(agent, \"calculator\", expression=\"2 + 2 * 3\")\n",
    "\n",
    "# Debug weather tool\n",
    "debug_tool_execution(agent, \"weather\", location=\"New York\")"
   ]
