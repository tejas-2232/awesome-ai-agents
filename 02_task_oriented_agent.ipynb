   "source": [
    "## Task-Oriented Agent Class"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "class TaskOrientedAgent:\n",
    "    \"\"\"AI Agent that can execute tasks using various tools\"\"\"\n",
    "    \n",
    "    def __init__(self, model=\"gpt-3.5-turbo\"):\n",
    "        self.model = model\n",
    "        self.tools = {}\n",
    "        self.tasks = []\n",
    "        self.system_prompt = \"\"\"\n",
    "        You are a task-oriented AI agent that helps users accomplish specific tasks.\n",
    "        You have access to various tools and can break down complex requests into smaller tasks.\n",
    "        \n",
    "        When a user makes a request:\n",
    "        1. Understand what they want to accomplish\n",
    "        2. Determine which tools you need to use\n",
    "        3. Break down the task into steps if necessary\n",
    "        4. Execute the required tools with appropriate parameters\n",
    "        5. Provide a clear summary of what was accomplished\n",
    "        \n",
    "        Always be helpful, precise, and informative in your responses.\n",
    "        \"\"\"\n",
    "    \n",
    "    def add_tool(self, tool: Tool):\n",
    "        \"\"\"Add a tool to the agent's toolkit\"\"\"\n",
    "        self.tools[tool.name] = tool\n",
    "    \n",
    "    def get_available_tools(self) -> List[Dict[str, Any]]:\n",
    "        \"\"\"Get information about available tools\"\"\"\n",
    "        return [\n",
    "            {\n",
    "                \"name\": tool.name,\n",
    "                \"description\": tool.description,\n",
    "                \"schema\": tool.get_schema()\n",
    "            }\n",
    "            for tool in self.tools.values()\n",
    "        ]\n",
    "    \n",
    "    def execute_tool(self, tool_name: str, **kwargs) -> Dict[str, Any]:\n",
    "        \"\"\"Execute a specific tool with given parameters\"\"\"\n",
    "        if tool_name not in self.tools:\n",
    "            return {\"success\": False, \"error\": f\"Tool '{tool_name}' not found\"}\n",
    "        \n",
    "        try:\n",
    "            result = self.tools[tool_name].execute(**kwargs)\n",
    "            return result\n",
    "        except Exception as e:\n",
    "            return {\"success\": False, \"error\": str(e)}\n",
    "    \n",
    "    def plan_and_execute(self, user_request: str) -> Dict[str, Any]:\n",
    "        \"\"\"Plan and execute tasks based on user request\"\"\"\n",
    "        # Create a prompt that includes available tools\n",
    "        tools_info = \"\\n\".join([\n",
    "            f\"- {tool['name']}: {tool['description']}\"\n",
    "            for tool in self.get_available_tools()\n",
    "        ])\n",
    "        \n",
    "        planning_prompt = f\"\"\"\n",
    "        {self.system_prompt}\n",
    "        \n",
    "        Available tools:\n",
    "        {tools_info}\n",
    "        \n",
    "        User request: {user_request}\n",
    "        \n",
    "        Please analyze this request and respond with a JSON object containing:\n",
    "        {{\n",
    "            \"understanding\": \"Brief description of what the user wants\",\n",
    "            \"plan\": [\"step 1\", \"step 2\", ...],\n",
    "            \"tools_needed\": [\n",
    "                {{\n",
    "                    \"tool\": \"tool_name\",\n",
    "                    \"parameters\": {{\"param1\": \"value1\"}}\n",
    "                }}\n",
    "            ]\n",
    "        }}\n",
    "        \n",
    "        If no tools are needed, leave tools_needed as an empty array.\n",
    "        \"\"\"\n",
    "        \n",
    "        try:\n",
    "            response = openai.ChatCompletion.create(\n",
    "                model=self.model,\n",
    "                messages=[\n",
    "                    {\"role\": \"user\", \"content\": planning_prompt}\n",
    "                ],\n",
    "                max_tokens=500,\n",
    "                temperature=0.1\n",
    "            )\n",
    "            \n",
    "            plan_text = response.choices[0].message.content\n",
    "            \n",
    "            # Extract JSON from the response\n",
    "            try:\n",
    "                # Find JSON in the response\n",
    "                json_start = plan_text.find('{')\n",
    "                json_end = plan_text.rfind('}') + 1\n",
    "                json_str = plan_text[json_start:json_end]\n",
    "                plan = json.loads(json_str)\n",
    "            except:\n",
    "                # Fallback if JSON parsing fails\n",
    "                plan = {\n",
    "                    \"understanding\": \"Could not parse plan\",\n",
    "                    \"plan\": [\"Respond to user request\"],\n",
    "                    \"tools_needed\": []\n",
    "                }\n",
    "            \n",
    "            # Execute the planned tools\n",
    "            execution_results = []\n",
    "            for tool_call in plan.get(\"tools_needed\", []):\n",
    "                tool_name = tool_call.get(\"tool\")\n",
    "                parameters = tool_call.get(\"parameters\", {})\n",
    "                \n",
    "                result = self.execute_tool(tool_name, **parameters)\n",
    "                execution_results.append({\n",
    "                    \"tool\": tool_name,\n",
    "                    \"parameters\": parameters,\n",
    "                    \"result\": result\n",
    "                })\n",
    "            \n",
    "            return {\n",
    "                \"success\": True,\n",
    "                \"plan\": plan,\n",
    "                \"execution_results\": execution_results,\n",
    "                \"raw_response\": plan_text\n",
    "            }\n",
    "            \n",
    "        except Exception as e:\n",
    "            return {\n",
    "                \"success\": False,\n",
    "                \"error\": str(e)\n",
    "            }\n",
    "    \n",
    "    def generate_response(self, user_request: str, execution_result: Dict[str, Any]) -> str:\n",
    "        \"\"\"Generate a natural language response based on execution results\"\"\"\n",
    "        if not execution_result.get(\"success\"):\n",
    "            return f\"I apologize, but I encountered an error: {execution_result.get('error', 'Unknown error')}\"\n",
    "        \n",
    "        plan = execution_result.get(\"plan\", {})\n",
    "        results = execution_result.get(\"execution_results\", [])\n",
    "        \n",
    "        response_parts = []\n",
    "        response_parts.append(f\"I understand you want to: {plan.get('understanding', 'accomplish your task')}\")\n",
    "        \n",
    "        if results:\n",
    "            response_parts.append(\"\\nHere's what I did:\")\n",
    "            for i, result in enumerate(results, 1):\n",
    "                tool_name = result[\"tool\"]\n",
    "                tool_result = result[\"result\"]\n",
    "                \n",
    "                if tool_result.get(\"success\"):\n",
    "                    if tool_name == \"calculator\":\n",
    "                        response_parts.append(f\"{i}. Calculated: {tool_result.get('expression')} = {tool_result.get('result')}\")\n",
    "                    elif tool_name == \"weather\":\n",
    "                        weather = tool_result.get(\"weather\", {})\n",
    "                        response_parts.append(f\"{i}. Weather in {tool_result.get('location')}: {weather.get('temperature')}Â°C, {weather.get('condition')}\")\n",
    "                    elif tool_name == \"file_writer\":\n",
    "                        response_parts.append(f\"{i}. {tool_result.get('message')}\")\n",
    "                    else:\n",
    "                        response_parts.append(f\"{i}. Used {tool_name}: {tool_result}\")\n",
    "                else:\n",
    "                    response_parts.append(f\"{i}. Error with {tool_name}: {tool_result.get('error')}\")\n",
    "        else:\n",
    "            response_parts.append(\"\\nNo tools were needed for this request.\")\n",
    "        \n",
    "        return \"\\n\".join(response_parts)\n",
    "    \n",
    "    def process_request(self, user_request: str) -> str:\n",
    "        \"\"\"Process a user request end-to-end\"\"\"\n",
    "        execution_result = self.plan_and_execute(user_request)\n",
    "        response = self.generate_response(user_request, execution_result)\n",
    "        return response"
   ]
  },
