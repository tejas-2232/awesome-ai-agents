   "source": [
    "## Task and Tool Definitions"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "@dataclass\n",
    "class Task:\n",
    "    \"\"\"Represents a task to be executed by the agent\"\"\"\n",
    "    name: str\n",
    "    description: str\n",
    "    parameters: Dict[str, Any]\n",
    "    status: str = \"pending\"  # pending, in_progress, completed, failed\n",
    "    result: Optional[Any] = None\n",
    "    error: Optional[str] = None\n",
    "\n",
    "class Tool:\n",
    "    \"\"\"Base class for agent tools\"\"\"\n",
    "    def __init__(self, name: str, description: str):\n",
    "        self.name = name\n",
    "        self.description = description\n",
    "    \n",
    "    def execute(self, **kwargs) -> Dict[str, Any]:\n",
    "        \"\"\"Execute the tool with given parameters\"\"\"\n",
    "        raise NotImplementedError\n",
    "    \n",
    "    def get_schema(self) -> Dict[str, Any]:\n",
    "        \"\"\"Return the tool's parameter schema\"\"\"\n",
    "        raise NotImplementedError"
   ]
  },
  {
   "cell_type": "markdown",
   "metadata": {},
   "source": [
    "## Example Tools Implementation"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "class CalculatorTool(Tool):\n",
    "    \"\"\"Simple calculator tool for mathematical operations\"\"\"\n",
    "    \n",
    "    def __init__(self):\n",
    "        super().__init__(\n",
    "            name=\"calculator\",\n",
    "            description=\"Perform basic mathematical calculations\"\n",
    "        )\n",
    "    \n",
    "    def execute(self, expression: str) -> Dict[str, Any]:\n",
    "        try:\n",
    "            # Basic safety check - only allow numbers, operators, and parentheses\n",
    "            if re.match(r'^[0-9+\\-*/().\\s]+$', expression):\n",
    "                result = eval(expression)\n",
    "                return {\"success\": True, \"result\": result, \"expression\": expression}\n",
    "            else:\n",
    "                return {\"success\": False, \"error\": \"Invalid expression\"}\n",
    "        except Exception as e:\n",
    "            return {\"success\": False, \"error\": str(e)}\n",
    "    \n",
    "    def get_schema(self) -> Dict[str, Any]:\n",
    "        return {\n",
    "            \"type\": \"object\",\n",
    "            \"properties\": {\n",
    "                \"expression\": {\n",
    "                    \"type\": \"string\",\n",
    "                    \"description\": \"Mathematical expression to evaluate\"\n",
    "                }\n",
    "            },\n",
    "            \"required\": [\"expression\"]\n",
    "        }\n",
    "\n",
    "class WeatherTool(Tool):\n",
    "    \"\"\"Weather information tool (mock implementation)\"\"\"\n",
    "    \n",
    "    def __init__(self):\n",
    "        super().__init__(\n",
    "            name=\"weather\",\n",
    "            description=\"Get current weather information for a location\"\n",
    "        )\n",
    "    \n",
    "    def execute(self, location: str) -> Dict[str, Any]:\n",
    "        # Mock weather data (in real implementation, you'd call a weather API)\n",
    "        mock_data = {\n",
    "            \"new york\": {\"temperature\": 22, \"condition\": \"sunny\", \"humidity\": 65},\n",
    "            \"london\": {\"temperature\": 15, \"condition\": \"cloudy\", \"humidity\": 80},\n",
    "            \"tokyo\": {\"temperature\": 28, \"condition\": \"rainy\", \"humidity\": 90},\n",
    "        }\n",
    "        \n",
    "        location_key = location.lower()\n",
    "        if location_key in mock_data:\n",
    "            return {\n",
    "                \"success\": True,\n",
    "                \"location\": location,\n",
    "                \"weather\": mock_data[location_key]\n",
    "            }\n",
    "        else:\n",
    "            return {\n",
    "                \"success\": False,\n",
    "                \"error\": f\"Weather data not available for {location}\"\n",
    "            }\n",
    "    \n",
    "    def get_schema(self) -> Dict[str, Any]:\n",
    "        return {\n",
    "            \"type\": \"object\",\n",
    "            \"properties\": {\n",
    "                \"location\": {\n",
    "                    \"type\": \"string\",\n",
    "                    \"description\": \"City or location name\"\n",
    "                }\n",
    "            },\n",
    "            \"required\": [\"location\"]\n",
    "        }\n",
    "\n",
    "class FileWriterTool(Tool):\n",
    "    \"\"\"Tool to write content to files\"\"\"\n",
    "    \n",
    "    def __init__(self):\n",
    "        super().__init__(\n",
    "            name=\"file_writer\",\n",
    "            description=\"Write content to a text file\"\n",
    "        )\n",
    "    \n",
    "    def execute(self, filename: str, content: str, mode: str = \"w\") -> Dict[str, Any]:\n",
    "        try:\n",
    "            with open(filename, mode) as f:\n",
    "                f.write(content)\n",
    "            return {\n",
    "                \"success\": True,\n",
    "                \"filename\": filename,\n",
    "                \"message\": f\"Content written to {filename}\"\n",
    "            }\n",
    "        except Exception as e:\n",
    "            return {\n",
    "                \"success\": False,\n",
    "                \"error\": str(e)\n",
    "            }\n",
    "    \n",
    "    def get_schema(self) -> Dict[str, Any]:\n",
    "        return {\n",
    "            \"type\": \"object\",\n",
    "            \"properties\": {\n",
    "                \"filename\": {\n",
    "                    \"type\": \"string\",\n",
    "                    \"description\": \"Name of the file to write to\"\n",
    "                },\n",
    "                \"content\": {\n",
    "                    \"type\": \"string\",\n",
    "                    \"description\": \"Content to write to the file\"\n",
    "                },\n",
    "                \"mode\": {\n",
    "                    \"type\": \"string\",\n",
    "                    \"description\": \"File mode (w for write, a for append)\",\n",
    "                    \"default\": \"w\"\n",
    "                }\n",
    "            },\n",
    "            \"required\": [\"filename\", \"content\"]\n",
    "        }"
   ]
