    "def interactive_task_session(agent, max_tasks=3):\n",
    "    \"\"\"\n",
    "    Start an interactive session with the task-oriented agent.\n",
    "    \"\"\"\n",
    "    print(\"\\n=== Interactive Task Session ===\")\n",
    "    print(\"I can help you with calculations, weather information, and file operations.\")\n",
    "    print(\"Type 'quit' to end the session.\")\n",
    "    print(\"-\" * 50)\n",
    "    \n",
    "    task_count = 0\n",
    "    while task_count < max_tasks:\n",
    "        try:\n",
    "            user_input = input(f\"\\nTask {task_count + 1}/{max_tasks}: \")\n",
    "            \n",
    "            if user_input.lower() in ['quit', 'exit', 'done']:\n",
    "                print(\"Session ended. Have a great day!\")\n",
    "                break\n",
    "            \n",
    "            response = agent.process_request(user_input)\n",
    "            print(f\"\\nAgent: {response}\")\n",
    "            \n",
    "            task_count += 1\n",
    "            \n",
    "        except KeyboardInterrupt:\n",
    "            print(\"\\nSession interrupted by user.\")\n",
    "            break\n",
    "        except Exception as e:\n",
    "            print(f\"Error: {e}\")\n",
    "            break\n",
    "\n",
    "# Uncomment to run interactive session\n",
    "# interactive_task_session(agent)"
   ]
